<%- include('partials/header') -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <input type="text" name="compositionTitle" class="form-control" placeholder="Title" autocomplete="off">
    </div>
    <div class="form-group">
        <textarea class="form-control" name="compositionBody" rows="3" placeholder="Write Your Content Here ..." autocomplete="off"></textarea>
      </div>
    <button type="submit" name="publishBtn" class="btn btn-primary btn-sm">Publish</button>
</form>
<%- include('partials/footer') -%>